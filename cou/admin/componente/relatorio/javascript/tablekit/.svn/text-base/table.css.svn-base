
.sortcol {
	background-position:right center;
	background-repeat:no-repeat;
	background-color:#EEE;
	cursor:pointer;
}
.sortasc {
	background-color:#DDFFAC;
	background-image:url(/componente/relatorio/javascript/tablekit/up.gif);
}
.sortdesc {
	background-color:#B9DDFF;
	background-image:url(/componente/relatorio/javascript/tablekit/down.gif);
}
.nosort {
	cursor:default;
}
